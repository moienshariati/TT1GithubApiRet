<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
    android:background="@color/cardview_dark_background"

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ShowGitResult">

    <LinearLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
      android:layout_gravity="center"
        >
        <TextView
            android:gravity="center"
            android:textColor="#333333"
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Git Client Information"
            android:textSize="28dp"
            android:fontFamily="@font/flyfont"
            android:background="@drawable/myshape1"
        android:textAllCaps="false"

            />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"

        android:orientation="horizontal">

        <TextView

            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="16dp"
            android:background="@drawable/myshape1"
            android:drawableLeft="@drawable/ic_account_circle_white_24dp"
            android:fontFamily="@font/fontroboto"
            android:gravity="end"
            android:padding="8dp"
            android:singleLine="true"
            android:text="Username:"
            android:textColor="#333333"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/tv_shoname"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:background="@drawable/myshape1"
            android:fontFamily="@font/fontroboto"
            android:gravity="center"
            android:padding="8dp"
            android:text=" "
            android:textColor="#ffffff"
            android:textSize="20dp"


            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"

        android:orientation="horizontal">

        <TextView
            android:gravity="left"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="@drawable/myshape1"
            android:drawableLeft="@drawable/ic_info_white_32dp"
       android:drawablePadding="2dp"

            android:fontFamily="@font/fontroboto"
            android:padding="8dp"
            android:layout_marginLeft="16dp"
            android:text="Bio:"
            android:textColor="#333333"
            android:textSize="20dp"
            android:singleLine="true"
            />

        <TextView
            android:id="@+id/textview_bio"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:background="@drawable/myshape1"
            android:fontFamily="@font/fontroboto"
            android:gravity="center"
            android:padding="8dp"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="20dp"


            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"

        android:orientation="horizontal">

        <TextView
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="16dp"
            android:background="@drawable/myshape1"
            android:drawableLeft="@drawable/ic_location_on_white_32dp"
            android:drawablePadding="2dp"

            android:fontFamily="@font/fontroboto"
            android:gravity="left"
            android:padding="8dp"
            android:singleLine="true"
            android:text="Location:"
            android:textColor="#333333"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/tv_sholocation"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:background="@drawable/myshape1"
            android:fontFamily="@font/fontroboto"
            android:gravity="center"
            android:padding="8dp"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="20dp"


            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"

        android:orientation="horizontal">

        <TextView
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="16dp"
            android:background="@drawable/myshape1"
            android:drawableLeft="@drawable/ic_location_city_black_24dp"
            android:drawablePadding="2dp"

            android:fontFamily="@font/fontroboto"
            android:gravity="left"
            android:padding="8dp"
            android:singleLine="true"
            android:text="Company:"
            android:textColor="#333333"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/tv_shocompany"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:background="@drawable/myshape1"
            android:fontFamily="@font/fontroboto"
            android:gravity="center"
            android:padding="8dp"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="20dp"


            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@android:color/white" />
        <LinearLayout
            android:layout_margin="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            >
            <TextView
                android:gravity="center"
                android:textColor="#333333"
                android:padding="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="More information"
                android:textSize="28dp"
                android:fontFamily="@font/flyfont"
                android:background="@drawable/myshape1"
                android:textAllCaps="false"

                />
        </LinearLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
            android:layout_margin="15dp"
            android:padding="8dp"
            android:id="@+id/tv_github"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:background="@drawable/myshape1"
            android:fontFamily="@font/fontroboto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        </ScrollView>
    </LinearLayout>
</LinearLayout><!--    <TextView-->
